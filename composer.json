    "description": "A Laravel 5 Page model, migration and controller",
    "license": "MIT",
    "keywords": [ "php", "laravel", "pages", "larapages" ],
    "homepage": "http://www.larapages.nl",
    "authors": [
        {
            "name": "Nick de Kruijk",
            "email": "git@larapages.nl"
        }
